{
  "version": "1.0",
  "projections": {
    "leaderboard-server": {
      "transforms": [
        {
          "name": "includeServices",
          "args": {
            "services": [
              "kotlinconf#ElasticLeaderboardService"
            ]
          }
        }
      ],
      "plugins": {
        "rust-server-codegen": {
          "module": "leaderboard-server-sdk",
          "moduleVersion": "1.0.0",
          "moduleAuthors": ["no-reply@amazon.com"]
        }
      }
    }
  }
}